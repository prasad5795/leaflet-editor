{
    "name": "tori-validator",
    "version": "2.1.22",
    "private": true,
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.22",
        "@fortawesome/free-solid-svg-icons": "^5.10.2",
        "@fortawesome/react-fontawesome": "^0.1.4",
        "@geoman-io/leaflet-geoman-free": "^2.3.0",
        "@material-ui/core": "^4.3.3",
        "@turf/turf": "^5.1.6",
        "@types/leaflet-editable": "^0.7.34",
        "axios": "^0.19.0",
        "connected-react-router": "^6.5.2",
        "font-awesome": "^4.7.0",
        "jwt-decode": "^2.2.0",
        "leaflet": "^1.5.1",
        "leaflet-draw": "^1.0.4",
        "leaflet-editable": "^1.2.0",
        "leaflet-routing-machine": "^3.2.12",
        "leaflet.pm-snapignore": "^1.0.2",
        "mathjs": "^7.5.1",
        "react": "^16.9.0",
        "react-d3-library": "^1.1.8",
        "react-dom": "^16.9.0",
        "react-leaflet": "^2.4.0",
        "react-leaflet-control": "^2.1.1",
        "react-leaflet-draw": "^0.19.0",
        "react-notify-toast": "^0.5.0",
        "react-redux": "^7.1.1",
        "react-router": "^5.0.1",
        "react-router-dom": "^5.0.1",
        "react-scripts": "^3.1.1",
        "react-test-renderer": "^16.9.0",
        "recompose": "^0.30.0",
        "redux": "^4.0.4",
        "redux-saga": "^1.0.5"
    },
    "scripts": {
        "start": "react-scripts start",
        "build:prod": "react-scripts build",
        "build:dev": "dotenv -e .env.development react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "version": "echo $npm_package_version"
    },
    "eslintConfig": {
        "env": {
            "browser": true,
            "es6": true
        },
        "extends": [
            "eslint:recommended",
            "plugin:react/recommended"
        ],
        "globals": {
            "Atomics": "readonly",
            "SharedArrayBuffer": "readonly"
        },
        "parserOptions": {
            "ecmaFeatures": {
                "jsx": true
            },
            "ecmaVersion": 2018,
            "sourceType": "module"
        },
        "plugins": [
            "react"
        ],
        "rules": {
            "react/prop-types": "off",
            "no-console": "off",
            "indent": [
                "error",
                2
            ],
            "linebreak-style": [
                "error",
                "unix"
            ],
            "quotes": [
                "error",
                "single"
            ],
            "semi": [
                "error",
                "never"
            ]
        },
        "settings": {
            "react": {
                "pragma": "React",
                "version": "detect"
            }
        }
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "homepage": "https://tori-production.maps-pu-core-prod.amiefarm.com/tori/",
    "devDependencies": {
        "@redux-saga/testing-utils": "^1.0.2",
        "@testing-library/react": "^9.3.0",
        "dotenv-cli": "^2.0.1",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.11.2",
        "nock": "^10.0.2",
        "redux-devtools-extension": "^2.13.8",
        "redux-mock-store": "^1.5.3"
    }
}
